{
  "buildCommand": "pip install -r requirements-vercel.txt",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60,
      "memory": 3008
    }
  }
}

